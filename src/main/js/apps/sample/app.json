                        "basemap": "gray-vector"
                    },
                    {
                        "id": "esri_hybrid",
                        "title": "Luftbild (hybrid)",
                        "thumbnailUrl": "resource('${app}:/images/hybrid.png')",
                        "basemap": "hybrid"
                    }
                ],
                "map": {
                    "layers": [
                        {
                            "id": "koeln",
                            "url": "https://services.conterra.de/arcgis/rest/services/common/koeln/MapServer",
                            "type": "AGS_DYNAMIC",
                            "title": "${map.cologne}",
                            "initiallyExpandedInToc": true,
                            "sublayers": [
                                {
                                    "id": 0,
                                    "initiallyExpandedInToc": true,
                                    "sublayers": [
                                        {
                                            "id": 1
                                        },
                                        {
                                            "id": 2,
                                            "visible": false
                                        },
                                        {
                                            "id": 3,
                                            "visible": false
                                        }
                                    ]
                                },
                                {
                                    "id": 4,
                                    "visible": false,
                                    "initiallyExpandedInToc": true
                                },
                                {
                                    "id": 8,
                                    "visible": false,
                                    "initiallyExpandedInToc": true
                                }
                            ]
                        }
                    ]
                },
                "view": {
                    "viewmode": "2D",
                    "center": {
                        "x": 473033.305195,
                        "y": 5682079.732658,
                        "spatialReference": 3857
                    },
                    "scale": 140000
                }
            }
        },
        "themes": {
            "ThemeModel": {
                "_selectedTheme": "autumn"
            },
            "ThemeSelector": {
                "componentEnabled": true
            }
        },
        "toc": {
            "Config": {
                "showBasemaps": true,
                "showLayerLegend": true
            }
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": [
                    {
                        "id": "mapview_tools",
                        "tools": [
                            "mapControlsToggleTool",
                            "viewmodeSwitcherTool",
                            "locateMeTool",
                            "zoomInTool",
                            "zoomOutTool",
                            "compassTool",
                            "restoreInitialViewTool",
                            "viewmodeSwitcherTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "mapview_tools"
                        },
                        "container": "ignore",
                        "windowType": "container",
                        "cssClass": "muted",
                        "tooltipPositions": [
                            "before",
                            "above",
                            "below",
                            "after"
                        ]
                    },
                    {
                        "id": "drawer_left",
                        "title": "Werkzeuge",
                        "cssClass": "ct-main-app-menu",
                        "tools": [
                            "tocToggleTool",
                            "printingToggleTool",
                            "sharelinkTool",
                            "helloTool",
                            "IMPRINT_Imprint_CustomInfoTool",
                            "IMPRINT_Imprint_CustomInfoTool_mobile"
                        ],
                        "registerWidget": {
                            "widgetRole": "drawer_button"
                        },
                        "container": "ignore",
                        "windowType": "drawer_left",
                        "tooltipPositions": [
                            "after",
                            "above",
                            "below",
                            "before"
                        ]
                    }
                ]
            }
        },
        "toolrules": {
            "ToolActiveStateManager": {
                "activateOnStartToolIds": [
                    "mapControlsToggleTool"
                ]
            }
        }
